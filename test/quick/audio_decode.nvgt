void main() {
	audio_decoder d;
	if(!d.open(file("../data/audio/points.ogg", "rb"))) {
		alert("can't open", SOUNDSYSTEM_LAST_ERROR);
		exit();
	}
	sound s;
	s.stream_pcm(d.read(1024 * 256), d.sample_rate, d.channels);
	wait(1000);
}

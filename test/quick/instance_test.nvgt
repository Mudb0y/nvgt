#include "instance.nvgt"
instance i("testapp");
void main() {
	show_window("test");
	while (!key_down(KEY_ESCAPE)) {
		wait(5);
		if (key_pressed(KEY_SPACE))
			screen_reader_speak(i.is_already_running ? "running" : "not running", true);
	}
}

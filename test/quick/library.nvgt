void main() {
	library l;
	if (!l.load("user32")) {
		alert("error", "can't load library");
		return;
	}
	dictionary@ d = l.call("int MessageBoxW(int, ptr, ptr, int)", 0, string_recode("this is a test", "UTF8", "UTF16"), string_recode("no way this will work", "UTF8", "UTF16"), 1);
	int r = -1;
	d.get(0, r);
	alert("test", r);
}

void main() {
	tts_voice v;
	show_window("test");
	while (!key_down(KEY_ESCAPE)) {
		wait(5);
		if (key_pressed(KEY_NUMPAD_ENTER)) v.speak_interrupt("NPEnter");
		if (key_pressed(KEY_NUMPAD_5)) v.speak_interrupt("NP5");
	}

}
